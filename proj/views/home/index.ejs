        <script type="text/javascript">
        function learnRegExp(s) {    
            var regexp = /(\w+:{0,1}\w*@)?(\S+)(:[0-9]+)?(\/|\/([\w#!:.?+=&%@!\-\/]))?/;
            return regexp.test(s);    
        }
        $(function(){ 
            
            $('#button-new').click(function(){
                console.log('Entered');

                data = document.getElementById("name").value;
                $('app-idea').html('<a href="'+data+'">link</a>');
                if(!learnRegExp(data))
                {
                    alert(data+'is not a valid url');
                    return;
                }
                console.log(data);
                esc_data = encodeURIComponent(data);
                $.ajax({
                    url:"http://localhost:1337/api/url/"+esc_data,
                    type:'POST',
                    success:function(retdata){
                        $('#class-container').html(data);
                    }
                });
            });
        });
</script>
